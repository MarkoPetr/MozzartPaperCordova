{
  "name": "mozzart-paper-cordova",
  "version": "1.0.0",
  "private": true,
  "description": "Cordova app for viewing screenshots as digital paper",
  "scripts": {
    "build": "echo 'Cordova build handled by Appflow'"
  },
  "devDependencies": {
    "cordova-android": "^14.0.1",
    "cordova-plugin-camera": "^8.0.0",
    "cordova-plugin-file": "^8.1.3"
  },
  "cordova": {
    "platforms": [
      "android"
    ],
    "plugins": {
      "cordova-plugin-camera": {
        "ANDROIDX_CORE_VERSION": "1.6.+"
      },
      "cordova-plugin-file": {
        "ANDROIDX_WEBKIT_VERSION": "1.4.0"
      }
    }
  }
}
